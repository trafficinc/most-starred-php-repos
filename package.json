        "@babel/preset-typescript": "7.12.16",
        "babel-loader": "8.2.2",
        "clean-webpack-plugin": "^4.0.0",
        "ts-loader": "^9.2.5",
        "typescript": "^4.1.5",
        "webpack": "5.21.2",
